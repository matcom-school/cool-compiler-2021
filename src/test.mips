.data 
arra:   .word 4 5 6 7 0

.text
main:
li $a1 , 5                     #guarde 5 numero a buscar
la $s2 , arra                   #dir del array 
move		$a0, $a1			# $a0 = $a1
li			$v0, 1				# syscall print int
syscall							# execute

jal		 Contain				# jump to Contain and save position to $ra

move		$a0, $a1			# $a0 = $a1
li			$v0, 1				# syscall print int
syscall							# execute

move $a0 , $s0
li			$v0, 1				# syscall print str
syscall	
						# execute
move		$a0, $a1			# $a0 = $a1
li			$v0, 1				# syscall print int
syscall							# execute

li			$v0,10
syscall


Contain:
#s2 array
loop:
    lw $s0 , ($s2)     
    beq	$s0, $a1, Equal	# if $s0 ==a$t1 then target
    beq $s0 , $zero, END
    add $s2,$s2,4
    j loop
    

Equal:
li $s0 , 1
jr $ra

END:
li $s0 , 0
jr $ra